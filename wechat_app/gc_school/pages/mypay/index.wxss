.we7-bottom {
    display: flex;
    overflow: hidden;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    padding: 10rpx 0;
    border-top: 1rpx solid #fff;
}

.we7-bottom,.we7-bottom.active,.we7-bottom.active navigatorurl {
    background-color: #fff;
}

.we7-bottom .we7-bottom-item {
    flex: 1;
    padding: 5rpx;
    text-align: center;
}

.we7-bottom .item-img {
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
}

.we7-bottom .item-text {
    display: block;
    font-size: 12px;
    line-height: 1;
}

.container {
    font-size: 12px;
    justify-content: space-between;
    box-sizing: border-box;
}

.container,.modal {
    display: flex;
    flex-direction: column;
}

.modal {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.5);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 50000;
    align-items: center;
    justify-content: center;
    text-align: center;
    font-size: 28rpx;
}

.m_box {
    background: #fff;
    border-radius: 10rpx;
    width: 70%;
    padding: 20rpx 0 10rpx;
}

.border_top_2 {
    margin-top: 10rpx;
    padding-top: 10rpx;
    border-top: 2rpx solid #f3f3f3;
}

.m_btn {
    background: #fff;
    border: none;
    color: #199fb1;
    line-height: 2em;
}

.m_btn::after {
    border: none;
    width: 0;
    height: 0;
}

.page {
    background-color: #fbfbfb;
}

.list {
    padding: 15rpx;
}

.list .item {
    flex-wrap: wrap;
    background-color: #fff;
    padding: 15rpx;
}

.list .item,.list .item .title {
    display: flex;
    margin-bottom: 10rpx;
}

.list .item .title {
    font-size: 28rpx;
    flex: 1;
    justify-content: space-between;
}

.list .item .desc {
    width: 100%;
    color: #999;
    margin-bottom: 10rpx;
    font-size: 26rpx;
}

.list .item .footer {
    width: 100%;
    color: #ccc;
    font-size: 22rpx;
    display: flex;
}

.list .item .footer,.list .item .footer .sm {
    justify-content: center;
}

.list .item .footer .sm {
    flex: 1;
    text-align: center;
}

.list .item .footer .sm text {
    color: #64edac;
}

.list .item .footer .sm.none {
    flex: none;
}

.list .item .footer .sm:first-child {
    text-align: left;
}