.we7-bottom {
    display: flex;
    overflow: hidden;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    padding: 10rpx 0;
    border-top: 1rpx solid #fff;
}

.we7-bottom,.we7-bottom.active,.we7-bottom.active navigatorurl {
    background-color: #fff;
}

.we7-bottom .we7-bottom-item {
    flex: 1;
    padding: 5rpx;
    text-align: center;
}

.we7-bottom .item-img {
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
}

.we7-bottom .item-text {
    display: block;
    font-size: 12px;
    line-height: 1;
}

.container {
    font-size: 12px;
    justify-content: space-between;
    box-sizing: border-box;
}

.container,.modal {
    display: flex;
    flex-direction: column;
}

.modal {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.5);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 50000;
    align-items: center;
    justify-content: center;
    text-align: center;
    font-size: 28rpx;
}

.m_box {
    background: #fff;
    border-radius: 10rpx;
    width: 70%;
    padding: 20rpx 0 10rpx;
}

.border_top_2 {
    margin-top: 10rpx;
    padding-top: 10rpx;
    border-top: 2rpx solid #f3f3f3;
}

.m_btn {
    background: #fff;
    border: none;
    color: #199fb1;
    line-height: 2em;
}

.m_btn::after {
    border: none;
    width: 0;
    height: 0;
}

.contents {
    padding: 70rpx 0 30rpx;
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    background: #ff6444;
}

.bgmain {
    background-color: #fff!important;
}

.loadmore {
    display: flex;
    justify-content: center;
}

.loadmore image {
    width: 50rpx;
    height: 50rpx;
    margin-top: 20rpx;
}

.loadmore view {
    color: #bdb8b8;
    font-size: 30rpx;
}

page {
    background: #fff;
}

.slide-image {
    height: 100%;
    width: 100%;
    display: inline-block;
    overflow: hidden;
    border-radius: 50rpx;
}

.swiper-box {
    padding: 10rpx 20rpx;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.space {
    width: 100%;
    height: 20rpx;
    background: #eee;
}

.kind_contain {
    height: 90rpx;
    border-bottom: 1rpx solid #eee;
    background: #fff;
    align-items: center;
    position: relative;
    z-index: 3;
    overflow-x: scroll;
    margin: 0 20rpx;
}

.kind_contain,.nav {
    display: flex;
}

.nav {
    width: calc(100% - 180rpx);
    height: 100%;
    box-sizing: border-box;
}

.nav,.nav-item {
    white-space: nowrap;
}

.nav-item {
    display: inline-block;
    font-size: 32rpx;
    margin-right: 20rpx;
}

.nav-text {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    letter-spacing: 4rpx;
    box-sizing: border-box;
}

.tab-on {
    color: #7acfa6;
    font-size: 32rpx!important;
    font-weight: 600;
}

.kind_img {
    width: 60rpx;
    height: 60rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.kind_img image {
    width: 32rpx;
    height: 32rpx;
}

.kindlist_box {
    position: absolute;
    width: 100%;
    top: 90rpx;
    left: 0rpx;
    border-bottom: 1rpx solid #eee;
    z-index: 3;
}

.kindlist_card {
    padding: 20rpx 20rpx 0;
    box-sizing: border-box;
    background: #fff;
}

.kindlist_card,.list_grid {
    width: 100%;
    display: flex;
}

.list_grid {
    flex-wrap: wrap;
}

.list_one {
    width: 25%;
    padding: 0 10rpx 20rpx;
    box-sizing: border-box;
}

.list_one,.list_one view {
    display: flex;
}

.list_one view {
    width: 100%;
    height: 60rpx;
    align-items: center;
    justify-content: center;
    border: 1rpx solid #eee;
    border-radius: 10rpx;
    font-size: 28rpx;
    letter-spacing: 2rpx;
}

.list-on {
    background: #7acfa6;
    color: #fff;
    border: none!important;
}

.list_box {
    width: 100%;
    display: flex;
    align-items: center;
    padding: 20rpx;
    box-sizing: border-box;
}

.list_poster {
    width: 150rpx;
    height: 150rpx;
    border-radius: 10rpx;
}

.list_content {
    height: 160rpx;
    width: calc(100% - 150rpx);
    justify-content: space-between;
    padding: 0 10rpx;
}

.list_content,.list_word {
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
}

.list_word {
    width: 100%;
}

.list_title {
    width: 100%;
    font-size: 32rpx;
    letter-spacing: 3rpx;
    line-height: 45rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-weight: 700;
}

.list_word .eye {
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    float: right;
    align-items: center;
}

.list_word .eye image {
    width: 40rpx;
    height: 40rpx;
    padding-right: 5px;
}

.viewnum {
    font-size: 12px;
}

.list_author {
    padding-top: 10rpx;
    width: 100%;
    font-size: 27rpx;
    letter-spacing: 3rpx;
    line-height: 40rpx;
    color: #818181;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.list_between {
    width: 100%;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #eee;
    padding-bottom: 10rpx;
    padding-top: 0rpx;
}

.list_price {
    font-size: 32rpx;
    color: red;
    margin-right: 20rpx;
}

.list_time {
    font-size: 28rpx;
    letter-spacing: 2rpx;
    color: #818181;
}

.list_between image {
    width: 45rpx;
    height: 45rpx;
    margin-left: 20px;
}

.nocontent {
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    box-sizing: border-box;
}

.nocontent image {
    width: 150rpx;
    height: 150rpx;
}

.blank_text {
    padding-top: 10rpx;
    font-size: 32rpx;
    letter-spacing: 2rpx;
    color: #c6c6c8;
}

.fixed {
    position: fixed;
    left: 0rpx;
    top: 0rpx;
}

.nofixed {
    height: 91rpx;
    width: 100%;
    background: #fff;
}

.indedx {
    color: #ff6444;
    font-size: 22rpx;
    padding: 10rpx 0;
}

.indedxs {
    color: #010101;
    font-size: 22rpx;
    margin-top: 10rpx;
    padding: -1rpx 0 10rpx;
    box-sizing: border-box;
}

#cuIcon-homefills {
    width: 48rpx!important;
    position: relative!important;
    height: 48rpx!important;
}

#cuIcon-homefills,#cuIcon-homefillt {
    display: block!important;
    margin: 0 auto 10rpx!important;
    text-align: center!important;
    font-size: 40rpx!important;
}

#cuIcon-homefillt {
    width: 72rpx!important;
    position: absolute!important;
    height: 72rpx!important;
    top: 0;
    left: 0;
}

.publish {
    bottom: 30%;
    background-color: #7acfa6;
}

.my,.publish {
    position: fixed;
    right: 20rpx;
    width: 100rpx;
    height: 100rpx;
    text-align: center;
    line-height: 100rpx;
    border-radius: 50%;
    color: #fff;
}

.my {
    bottom: 20%;
    background-color: #ff6444;
}

.search {
    position: fixed;
    right: 20rpx;
    bottom: 40%;
    width: 100rpx;
    height: 100rpx;
    background-color: red;
    text-align: center;
    line-height: 100rpx;
    border-radius: 50%;
    color: #fff;
}