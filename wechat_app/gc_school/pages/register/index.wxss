.we7-bottom {
    display: flex;
    overflow: hidden;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    padding: 10rpx 0;
    border-top: 1rpx solid #fff;
}

.we7-bottom,.we7-bottom.active,.we7-bottom.active navigatorurl {
    background-color: #fff;
}

.we7-bottom .we7-bottom-item {
    flex: 1;
    padding: 5rpx;
    text-align: center;
}

.we7-bottom .item-img {
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
}

.we7-bottom .item-text {
    display: block;
    font-size: 12px;
    line-height: 1;
}

.container {
    font-size: 12px;
    justify-content: space-between;
    box-sizing: border-box;
}

.container,.modal {
    display: flex;
    flex-direction: column;
}

.modal {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.5);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 50000;
    align-items: center;
    justify-content: center;
    text-align: center;
    font-size: 28rpx;
}

.m_box {
    background: #fff;
    border-radius: 10rpx;
    width: 70%;
    padding: 20rpx 0 10rpx;
}

.border_top_2 {
    margin-top: 10rpx;
    padding-top: 10rpx;
    border-top: 2rpx solid #f3f3f3;
}

.m_btn {
    background: #fff;
    border: none;
    color: #199fb1;
    line-height: 2em;
}

.m_btn::after {
    border: none;
    width: 0;
    height: 0;
}

page {
    background-color: #fff!important;
}

.bgmain {
    background-color: #fff;
    color: #000;
}

.student {
    padding: 30rpx 30rpx 0;
}

.agreement {
    text-align: center;
    margin: 30rpx 0;
    font-size: 30rpx;
    display: flex;
    justify-content: center;
}

.agreement view {
    margin-left: 20rpx;
}

.agreement text {
    font-weight: 700;
}

.checkbox {
    transform: scale(.8,.8);
}

.rule {
    margin-left: 20rpx;
}

.getPhone {
    background: #17a9ee;
    color: #fff;
    line-height: 70rpx;
    font-size: 30rpx;
}